{
  "expo": {
    "name": "ticktok-clone-app",
    "slug": "ticktopcloneapp",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "sdkVersion": "37.0.0",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tiktok.app"
    },
    "android": {
      "package": "com.tiktok.app"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}